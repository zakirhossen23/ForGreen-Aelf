<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:local="clr-namespace:ForGreen_Aelf"
        xmlns:Layout="clr-namespace:ForGreen_Aelf.Componenet.Layout" x:Class="ForGreen_Aelf.MainWindow"
        mc:Ignorable="d"
        Title="ForGreen" Height="700" Width="1200" WindowStartupLocation="CenterScreen" Loaded="Window_Loaded" Icon="/Resources/favicon.png">
    <Grid Name="mainGrid" Background="#121F29" >
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="70"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>

            <Layout:Header x:Name="HeaderLayout" VerticalAlignment="Center" Height="70" />
            <Frame x:Name="MainFrame" Grid.Row="1"  NavigationUIVisibility="Hidden" />

        </Grid>
       
    </Grid>
</Window>
